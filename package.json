{
  "name": "parse-es6-example",
  "version": "1.0.0",
  "description": "Simple setup for Parse Cloud Code with Browserify + Babel.js",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gcollazo/parse-es6-example.git"
  },
  "scripts": {
    "postinstall": "npm run setup && npm run build",
    "setup": "mkdir -p cloud",
    "clean": "rm -rf cloud",
    "build": "browserify src/main.js -t babelify -o cloud/main.js",
    "deploy": "npm run build && parse deploy"
  },
  "author": "@gcollazo",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.0.1"
  }
}
