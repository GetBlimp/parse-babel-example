{
  "name": "parse-babel-example",
  "version": "1.0.0",
  "description": "Simple setup for Parse Cloud Code with Webpack + Babel.js",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gcollazo/parse-babel-example.git"
  },
  "scripts": {
    "postinstall": "npm run setup && npm run build",
    "setup": "mkdir -p cloud",
    "clean": "rm -rf cloud",
    "build": "webpack --progress --show-modules",
    "deploy": "npm run build && parse deploy"
  },
  "author": "@gcollazo",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "webpack": "^1.12.1"
  }
}
